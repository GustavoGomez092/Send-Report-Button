{
  "name": "El Botton de Carlos",
  "version": "1.0.0",
  "manifest_version": 2,
    "permissions": [
      "http://localhost:5000/*"
    ],
  "description": "Execute email sender API",
  "homepage_url": "http://gustavogomez.ga",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "browser action demo",
    "default_popup": "src/browser_action/browser_action.html"
  }
}